package tutorial4;

public class Main {

	final static int SECOND = 1000;
	
	public static void main(String[] args) {
		
		int a = 10;
		System.out.println("현지의 a는 " + a + "입니다.");
		a++;
		System.out.println("현지의 a는 " + a + "입니다.");
		System.out.println("현지의 a는 " + ++a + "입니다.");
		System.out.println("현지의 a는 " + a++ + "입니다.");
		System.out.println("현지의 a는 " + a + "입니다.");
		
	}

}
